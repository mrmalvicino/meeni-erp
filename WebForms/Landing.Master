<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Landing.master.cs" Inherits="WebForms.Landing" MasterPageFile="~/Site.master" %>

<asp:Content ContentPlaceHolderID="SiteHeadPlaceHolder" runat="server">
    <link href="style/landing-header.css" rel="stylesheet" />
    <asp:ContentPlaceHolder ID="LandingHeadPlaceHolder" runat="server">
    </asp:ContentPlaceHolder>
</asp:Content>

<asp:Content ContentPlaceHolderID="SiteHeaderPlaceHolder" runat="server">
    <header class="header-tag">
        <nav class="header-nav">
            <a href="Dashboard.aspx" class="invisible-link">
                <img data-image-id="logoImg" />
            </a>
            <div>
                <ul>
                    <li>
                        <a href="Login.aspx" class="light-button">
                            <i class="bi bi-key"></i>&nbsp<span>Iniciar sesión</span>
                        </a>
                    </li>
                    <li>
                        <a href="Signup.aspx" class="transparent-button">
                            <i class="bi bi-person-plus"></i>&nbsp Registrarse
                        </a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
</asp:Content>

<asp:Content ContentPlaceHolderID="SiteMainPlaceHolder" runat="server">
    <asp:ContentPlaceHolder ID="LandingMainPlaceHolder" runat="server">
    </asp:ContentPlaceHolder>
</asp:Content>
