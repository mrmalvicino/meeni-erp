        <div class="admin-menu-div">
            <nav class="width-90-pct">
                <ul>
                    <li>
                        <a href="Dashboard.aspx" class="dark-button width-100-pct margin-5-0-px big-hover">
                            <i class="bi bi-house"></i>&nbsp Inicio
                        </a>
                    </li>
                    <li>
                        <a href="Users.aspx" class="dark-button width-100-pct margin-5-0-px big-hover">
                            <i class="bi bi-person-badge"></i>&nbsp Usuarios
                        </a>
                    </li>
                    <li>
                        <a href="Clients.aspx" class="dark-button width-100-pct margin-5-0-px big-hover">
                            <i class="bi bi-people"></i>&nbsp Clientes
                        </a>
                    </li>
                    <li>
                        <a href="Suppliers.aspx" class="dark-button width-100-pct margin-5-0-px big-hover">
                            <i class="bi bi-truck"></i>&nbsp Proveedores
                        </a>
                    </li>
                    <li>
                        <a href="Warehouses.aspx" class="dark-button width-100-pct margin-5-0-px big-hover">
                            <i class="bi bi-buildings"></i>&nbsp Depósitos
                        </a>
                    </li>
                    <li>
                        <a href="Products.aspx" class="dark-button width-100-pct margin-5-0-px big-hover">
                            <i class="bi bi-box-seam"></i>&nbsp Productos
                        </a>
                    </li>
                    <li>
